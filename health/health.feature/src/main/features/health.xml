<?xml version="1.0" encoding="UTF-8"?>
<features name="uAAL Health Manager - ${pom.version}" xmlns="http://karaf.apache.org/xmlns/features/v1.0.0">
		<!-- Change version of repository accordingly -->
	<repository>mvn:org.universAAL.middleware/mw.karaf.feature/2.0.2-SNAPSHOT/xml/features</repository>
	<repository>mvn:org.universAAL.ontology/ont.karaf.feature/2.0.2-SNAPSHOT/xml/features</repository>
	
	<!-- Health Core -->
	<feature name="health.core" version="${platform.version}">
		<feature>uAAL-Ont.Profile.Health</feature>
		<bundle start-level='96'>mvn:org.universAAL.AALapplication/health.profile.manager/E.1.1-SNAPSHOT</bundle>
		<bundle start-level='96'>mvn:org.universAAL.AALapplication/health.treat.manager/E.1.1-SNAPSHOT</bundle>
		<bundle start-level='96'>mvn:org.universAAL.AALapplication/health.performedSession.manager/E.1.1-SNAPSHOT</bundle>
	</feature>
		
	<feature name="health.demo" version="${platform.version}">
		<feature>uAAL-Ont.Profile.Health</feature>
		<bundle start-level='96'>mvn:org.universAAL.AALapplication/health.demo.populator/E.1.1-SNAPSHOT</bundle>
		<bundle start-level='96'>mvn:org.universAAL.AALapplication/health.demo.populator.run/E.1.1-SNAPSHOT</bundle>
	</feature>
	
	<!-- Health UI -->
	<feature name="health.ui" version="${platform.version}">
		<feature>uAAL-Ont.Profile.Health</feature>
		<feature>uAAL-Ont.Recommendation</feature>
		<bundle start-level='96'>mvn:org.universAAL.AALapplication/health.ui.client/E.1.1-SNAPSHOT</bundle>
